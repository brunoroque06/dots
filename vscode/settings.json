{
  "[csharp]": {
    "editor.defaultFormatter": "csharpier.csharpier-vscode"
  },
  "[css][dockercompose][html][javascript][json][jsonc][markdown][scss][typescript][yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[csv]": {
    "editor.stickyScroll.enabled": true
  },
  "[python]": {
    "editor.defaultFormatter": "charliermarsh.ruff"
  },
  "breadcrumbs.symbolPath": "off",
  "chat.editor.fontSize": 14,
  "chat.mcp.gallery.enabled": true,
  "containers.containerClient": "com.microsoft.visualstudio.containers.podman",
  "containers.orchestratorClient": "com.microsoft.visualstudio.orchestrators.podmancompose",
  "D2.execPath": "/opt/homebrew/bin/d2",
  "debug.allowBreakpointsEverywhere": true,
  "debug.inlineValues": "on",
  "debug.internalConsoleOptions": "neverOpen",
  "debug.toolBarLocation": "docked",
  "diffEditor.ignoreTrimWhitespace": true,
  "diffEditor.renderSideBySide": false,
  "dotnet.automaticallyCreateSolutionInWorkspace": false,
  "dotnet.codeLens.enableReferencesCodeLens": false,
  "dotnet.formatting.organizeImportsOnFormat": true,
  "dotnet.inlayHints.enableInlayHintsForLiteralParameters": true,
  "dotnet.inlayHints.enableInlayHintsForOtherParameters": true,
  "dotnet.inlayHints.enableInlayHintsForParameters": true,
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.coverImportFormat": "always",
    "source.organizeImports": "always",
    "source.sort.json": "always"
  },
  "editor.cursorBlinking": "solid",
  "editor.cursorSurroundingLines": 5,
  "editor.cursorWidth": 2,
  "editor.fontFamily": "MPLUS1Code-Regular",
  "editor.fontSize": 14,
  "editor.formatOnSave": false,
  "editor.formatOnType": true,
  "editor.guides.bracketPairs": true,
  "editor.inlineSuggest.enabled": true,
  "editor.lineHeight": 1.45,
  "editor.lineNumbers": "relative",
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "trailing",
  "editor.scrollbar.verticalScrollbarSize": 8,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.sortOrder": "alphabetical",
  "explorer.openEditors.visible": 1,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "extensions.ignoreRecommendations": true,
  "files.autoSave": "afterDelay",
  "files.trimTrailingWhitespace": true,
  "git.openRepositoryInParentFolders": "never",
  "github.copilot.enable": {
    "editor": true,
    "inlineSuggest": true
  },
  "github.copilot.nextEditSuggestions.enabled": true,
  "github.copilot.nextEditSuggestions.fixes": true,
  "inlineChat.enableV2": true,
  "inlineChat.hideOnRequest": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.validate.enable": true,
  "markdown.preview.fontFamily": "SF Pro Rounded",
  "mssql.defaultQueryResultsViewMode": "Text",
  "mssql.format.keywordCasing": "lowercase",
  "mssql.intelliSense.lowerCaseSuggestions": true,
  "python.analysis.autoFormatStrings": true,
  "python.analysis.autoImportCompletions": true,
  "python.analysis.inlayHints.callArgumentNames": "partial",
  "python.analysis.inlayHints.functionReturnTypes": true,
  "python.analysis.inlayHints.pytestParameters": true,
  "python.analysis.inlayHints.variableTypes": true,
  "python.analysis.languageServerMode": "full",
  "python.analysis.typeCheckingMode": "strict",
  "python.analysis.typeEvaluation.strictDictionaryInference": true,
  "python.analysis.typeEvaluation.strictListInference": true,
  "python.analysis.typeEvaluation.strictSetInference": true,
  "python.defaultInterpreterPath": "/opt/homebrew/bin/python3",
  "python.languageServer": "Pylance",
  "python.terminal.activateEnvironment": false,
  "python.testing.pytestEnabled": true,
  "ruff.organizeImports": true,
  "scm.alwaysShowRepositories": true,
  "settingsSync.ignoredSettings": [
    "D2.execPath",
    "chat.editor.fontSize",
    "editor.fontFamily",
    "editor.fontSize",
    "editor.lineHeight",
    "markdown.preview.fontFamily"
  ],
  "terminal.integrated.defaultProfile.osx": "elvish",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.profiles.osx": {
    "elvish": {
      "path": "/opt/homebrew/bin/elvish"
    }
  },
  "terraform.mcp.server.enable": false,
  "tinymist.formatterMode": "typstyle",
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "typescript.inlayHints.variableTypes.enabled": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.commandCenter": false,
  "window.title": "${activeEditorLong}${separator}${rootPath}",
  "workbench.activityBar.location": "top",
  "workbench.colorTheme": "Default Light Modern",
  "workbench.editor.enablePreview": false,
  "workbench.editor.limit.perEditorGroup": true,
  "workbench.editor.limit.value": 1,
  "workbench.editor.revealIfOpen": false,
  "workbench.editor.showTabs": "single",
  "workbench.layoutControl.enabled": false,
  "workbench.panel.showLabels": false,
  "workbench.startupEditor": "none",
  "workbench.tree.indent": 15,
  "workbench.tree.renderIndentGuides": "always",
  "zig.zls.enabled": "on"
}
